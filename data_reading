#### Reading Data ####
# scrapping data from the website
require(rjson)
theURL <- "http://citibikenyc.com/stations/json"
r.data <- fromJSON(file = theURL)
# formatting data
time <- r.data$executionTime
ids <- sapply(r.data$stationBeanList, function(x) x$id)
a.docks <- sapply(r.data$stationBeanList, function(x) x$availableDocks)
a.bikes <- sapply(r.data$stationBeanList, function(x) x$availableBikes)
data <- data.frame(time, ids, a.docks, a.bikes)
# this is only data at one point
# how to get more?
